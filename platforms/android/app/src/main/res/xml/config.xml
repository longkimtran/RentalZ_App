<?xml version='1.0' encoding='utf-8'?>
<widget id="vn.edu.greenwich.database" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>RentalZ</name>
    <description>
        Tower Searching.
    </description>
    <author email="LongTHKGCS18852@fpt.edu.vn" href="https://greenwich.edu.vn/">
        Long Tran
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:+84962242999" />
    <allow-intent href="sms:+84962242999" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
